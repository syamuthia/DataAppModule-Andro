<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.komunikasidatajadi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".signup_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".Home" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".serial_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="landscape" />
        <activity android:name=".paralel_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".data_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".material_page" android:theme="@style/Theme.AppCompat.NoActionBar" android:screenOrientation="portrait"/>
    </application>

</manifest>